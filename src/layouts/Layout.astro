---
import Header from '@/components/Header/index.astro'
import Footer from '@/components/Footer/index.astro'
import Wrapper from '@/components/Wrapper/index.astro'

interface Props {
  lang: string
  title: string
}

const { lang, title } = Astro.props
---

<Wrapper lang={lang} title={title}>
  <Header />
  <slot />
  <Footer />
</Wrapper>
